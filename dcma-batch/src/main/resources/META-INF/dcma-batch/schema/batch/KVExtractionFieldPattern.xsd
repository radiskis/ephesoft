<xs:schema elementFormDefault="qualified"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="KVExtractionFieldPatterns">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="KVExtractionFieldPattern" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Distance" type="xs:string" />
							<xs:element name="FetchValue" type="xs:string" />
							<xs:element name="KeyPattern" type="xs:string" />
							<xs:element name="Length" type="xs:int" />
							<xs:element name="Location" type="xs:string" />
							<xs:element name="Multiplier" type="xs:float" default = "1"/>
							<xs:element name="NoOfWords" type="xs:int" />
							<xs:element name="ValuePattern" type="xs:string" />
							<xs:element name="Width" type="xs:int" />
							<xs:element name="XOffset" type="xs:int" />
							<xs:element name="YOffset" type="xs:int" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
